# TovPlay Frontend Staging Environment Variables

# API Configuration
VITE_API_BASE_URL=https://staging.tovplay.org/api
VITE_API_TIMEOUT=30000

# Environment
VITE_NODE_ENV=staging

# Feature Flags
VITE_ENABLE_DEBUG=true
VITE_ENABLE_ANALYTICS=false

# WebSocket Configuration  
VITE_WS_URL=ws://193.181.213.220:8001/ws

# Error Reporting
VITE_ENABLE_ERROR_REPORTING=true
VITE_ERROR_REPORTING_ENDPOINT=http://193.181.213.220:8001/api/errors

# Sentry Error Tracking
# Get your DSN from https://sentry.io after creating a frontend staging project
VITE_SENTRY_DSN=
VITE_ENV=staging
VITE_APP_VERSION=1.0.0-staging

# Google OAuth (placeholder for staging)
VITE_GOOGLE_CLIENT_ID=staging-google-client-id

VITE_DISCORD_INVITE_LINK=https://discord.gg/7y7n2Nfq